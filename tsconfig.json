{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "baseUrl": "./",
        "paths": {
            "@assets/*": ["./src/assets/*"],
            "@images/*": ["./src/assets/images/*"],
            "@icons/*": ["./src/assets/icons/*"],
            "@pages/*": ["./src/pages/*"],
            "@components/*": ["./src/components/*"],
            "@styles/*": ["./src/styles/*"],
            "@api/*": ["./src/api/*"],
            "@typings/*": ["./src/typings/*"],
            "@utils/*": ["./src/utils/*"],
            "@hooks/*": ["./src/hooks/*"],
        }
    }
}